                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT cttc.Ma,
       cttc.Ngay,
       cttc.Ngay_Ghi_So,
       cttc.Ma_Chung_Tu_Cam_Do,
       cttc.So_Tien_Thu,
       cttc.So_Tien_Chi,

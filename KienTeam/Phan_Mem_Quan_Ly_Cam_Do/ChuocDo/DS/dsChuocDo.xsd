                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>--DECLARE @FromDate DATETIME
--DECLARE @ToDate DATETIME
--
--SET @FromDate = '1/1/2000'
--SET @ToDate = '12/31/2015'

SELECT ctct.Ma_Chi_Tiet,
       ctct.Ma_Chung_Tu,
       ct.Ngay,
       ct.Ten_Khach_Hang,
       ct.So_CMND,
       ct.Dia_Chi,
       ct.So_Tien_Cam,
       ctct.Ten_Tai_San,
       ctct.Loai_Vang,
       ctct.Trong_Luong,
       ctct.Trong_Luong_Vang_18k,
       ctct.Trong_Luong_Vang_24k,
       ctct.Trong_Luong_Khac,
       ctct.Chuan_Do,
       ctct.Gia_Tri_Vat_Cam,
       ctct.Sap_Xep,
       CASE 
            WHEN cttc.Ma IS NULL THEN 'C'
            ELSE 'R'
       END AS Tinh_Trang
FROM   CHUNG_TU_CHI_TIET ctct
       LEFT OUTER JOIN CHUNG_TU ct
            ON  ct.Ma_Chung_Tu = ctct.Ma_Chung_Tu
       INNER JOIN (
                SELECT *
                FROM   CHUNG_TU_THU_CHI
                WHERE  Phan_Loai = N'Chuộc'
            ) cttc
            ON  ct.Ma_Chung_Tu = cttc.Ma_Chung_Tu_Cam_Do
WHERE  DATEDIFF(DAY, @FromDate, cttc.Ngay) &gt;= 0
       AND DATEDIFF(DAY, @ToDate, cttc.Ngay) &lt;= 0</CommandText>

          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="CHUNG_TU_Theo_NgayTableAdapter" GeneratorDataComponentClassName="CHUNG_TU_Theo_NgayTableAdapter" Name="CHUNG_TU_Theo_Ngay" UserDataComponentName="CHUNG_TU_Theo_NgayTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="Quan_Ly_Cam_DoConnectionString (Settings)" DbObjectName="Quan_Ly_Cam_Do.dbo.CHUNG_TU" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>--DECLARE @FromDate DATETIME
--DECLARE @ToDate DATETIME
--
--SET @FromDate = '1/1/2000'
--SET @ToDate = '12/31/2015'

SELECT ct.Ma_Chung_Tu,
       (
           ct.So_Tien_Cam - ISNULL(
               (
                   SELECT SUM(So_Tien_Thu - So_Tien_Chi)
                   FROM   CHUNG_TU_THU_CHI cttc
                   WHERE  cttc.Ma_Chung_Tu_Cam_Do = ct.Ma_Chung_Tu
                          AND cttc.Phan_Loai != N'Chuộc'
               ),
               0
           )
       ) AS So_Tien,
       ISNULL(
           (
               SELECT MAX(cttc.Ngay)
               FROM   CHUNG_TU_THU_CHI cttc
               WHERE  cttc.Ma_Chung_Tu_Cam_Do = ct.Ma_Chung_Tu
                      AND cttc.Phan_Loai = N'Chuộc'
           ),
           ct.Ngay
       ) AS Ngay_Chuoc,
       ISNULL(
           (
               SELECT SUM(cttc.So_Tien_Thu)
               FROM   CHUNG_TU_THU_CHI cttc
               WHERE  cttc.Ma_Chung_Tu_Cam_Do = ct.Ma_Chung_Tu
           ),
           0
       ) AS Tong_Thu,
       (
           ISNULL(
               (
                   SELECT SUM(cttc.So_Tien_Chi)
                   FROM   CHUNG_TU_THU_CHI cttc
                   WHERE  cttc.Ma_Chung_Tu_Cam_Do = ct.Ma_Chung_Tu
               ),
               0
           ) + ct.So_Tien_Cam
       ) AS Tong_Chi,
       (
           ISNULL(
               (
                   SELECT SUM(cttc.So_Tien_Thu - cttc.So_Tien_Chi)
                   FROM   CHUNG_TU_THU_CHI cttc
                   WHERE  cttc.Ma_Chung_Tu_Cam_Do = ct.Ma_Chung_Tu
               ),
               0
           ) - ct.So_Tien_Cam
       ) AS Tong_Chenh_Lech,
       ct.Ngay AS Ngay_Cam_Do,
       ct.So,
       ct.Ten_Khach_Hang,
       ct.So_CMND,
       ct.Ngay_Cap_CMND,
       ct.Noi_Cap,
       ct.Dia_Chi,
       ct.So_Dien_Thoai,
       ct.Chu_Tiem,
       ct.So_Tien_Cam,
       ct.Tu_Ngay,
       ct.Den_Ngay,
       ct.Lai_Suat_Ngay,
       ct.Lai_Suat_Thang,
       ct.Mat_Giay,
       ct.Ghi_Chu
FROM   CHUNG_TU ct
       INNER JOIN (
                SELECT *
                FROM   CHUNG_TU_THU_CHI
                WHERE  Phan_Loai = N'Chuộc'
            ) cttc
            ON  ct.Ma_Chung_Tu = cttc.Ma_Chung_Tu_Cam_Do
WHERE  DATEDIFF(DAY, @FromDate, cttc.Ngay) &gt;= 0
       AND DATEDIFF(DAY, @ToDate, cttc.Ngay) &lt;= 0</CommandText>

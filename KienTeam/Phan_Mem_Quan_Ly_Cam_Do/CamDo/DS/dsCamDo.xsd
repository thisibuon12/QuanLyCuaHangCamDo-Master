          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="CHUNG_TU_Theo_NgayTableAdapter" GeneratorDataComponentClassName="CHUNG_TU_Theo_NgayTableAdapter" Name="CHUNG_TU_Theo_Ngay" UserDataComponentName="CHUNG_TU_Theo_NgayTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="Quan_Ly_Cam_DoConnectionString (Settings)" DbObjectName="SELECT *, dbo.Lay_Thang(CHUNG_TU.Ngay_Ghi_So, GETDATE()) AS So_Thang_Cam, DATEDIFF(DAY, CHUNG_TU.Ngay_Cam_Do, GETDATE()) AS So_Ngay_Cam, dbo.Lay_Ngay(CHUNG_TU.Ngay_Ghi_So, GETDATE()) AS So_Ngay_Le FROM ( SELECT ct.Ma_Chung_Tu, ( ct.So_Tien_Cam - ISNULL( ( SELECT SUM(So_Tien_Thu - So_Tien_Chi) FROM CHUNG_TU_THU_CHI cttc WHERE cttc.Ma_Chung_Tu_Cam_Do = ct.Ma_Chung_Tu AND cttc.Ma_Phan_Loai != 'Chuoc' AND cttc.Ma_Phan_Loai != 'Loi' ), 0 ) ) AS So_Tien, ISNULL( ( SELECT MAX(cttc.Ngay_Ghi_So) FROM CHUNG_TU_THU_CHI cttc WHERE cttc.Ma_Chung_Tu_Cam_Do = ct.Ma_Chung_Tu ), ct.Ngay ) AS Ngay_Ghi_So, ct.Ngay AS Ngay_Cam_Do, ct.So, ct.Lien, ct.Ten_Khach_Hang, ct.So_CMND, ct.Ngay_Cap_CMND, ct.Noi_Cap, ct.Ngay_Sinh, ct.Dia_Chi, ct.So_Dien_Thoai, ct.Chu_Tiem, ct.So_Tien_Cam, ct.Tu_Ngay, ct.Den_Ngay, ct.Lai_Suat_Ngay, ct.Lai_Suat_Thang, ct.Mat_Giay, ct.Ghi_Chu, CASE WHEN cttc.Ma IS NULL THEN 'C' ELSE 'R' END AS Tinh_Trang, ct.Chung_Tu_Goc, ct.Da_Lam_Lai_Giay, ct.Sap_Xep FROM CHUNG_TU ct LEFT OUTER JOIN ( SELECT * FROM CHUNG_TU_THU_CHI WHERE Ma_Phan_Loai = 'Chuoc' ) cttc ON ct.Ma_Chung_Tu = cttc.Ma_Chung_Tu_Cam_Do WHERE DATEDIFF(DAY, @FromDate, ct.Ngay) &gt;= 0 AND DATEDIFF(DAY, @ToDate, ct.Ngay) &lt;= 0 AND ( (cttc.Ma IS NULL AND ISNULL(ct.Da_Lam_Lai_Giay, 0) = 0) OR CAST(1 AS BIT) = @IsAll ) ) CHUNG_TU" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>--DECLARE @FromDate DATETIME
--DECLARE @ToDate DATETIME
--DECLARE @IsAll BIT
--
--SET @FromDate = '1/1/2000'
--SET @ToDate = '12/31/2020'
--SET @IsAll = 1

SELECT DANH_SACH_CHUNG_TU.Ma_Chung_Tu,
       DANH_SACH_CHUNG_TU.So_Tien,
       DANH_SACH_CHUNG_TU.Ngay_Ghi_So,
       DANH_SACH_CHUNG_TU.Ngay_Cam_Do,
       DANH_SACH_CHUNG_TU.So,
       DANH_SACH_CHUNG_TU.Lien,
       DANH_SACH_CHUNG_TU.Ten_Khach_Hang,
       DANH_SACH_CHUNG_TU.So_CMND,
       DANH_SACH_CHUNG_TU.Ngay_Cap_CMND,
       DANH_SACH_CHUNG_TU.Noi_Cap,
       DANH_SACH_CHUNG_TU.Ngay_Sinh,
       DANH_SACH_CHUNG_TU.Dia_Chi,
       DANH_SACH_CHUNG_TU.So_Dien_Thoai,
       DANH_SACH_CHUNG_TU.Chu_Tiem,
       DANH_SACH_CHUNG_TU.So_Tien_Cam,
       DANH_SACH_CHUNG_TU.Tu_Ngay,
       DANH_SACH_CHUNG_TU.Den_Ngay,
       DANH_SACH_CHUNG_TU.Lai_Suat_Ngay,
       DANH_SACH_CHUNG_TU.Lai_Suat_Thang,
       DANH_SACH_CHUNG_TU.Ghi_Chu,
       DANH_SACH_CHUNG_TU.Mat_Giay,
       DANH_SACH_CHUNG_TU.So_Ngay_Cam,
       DANH_SACH_CHUNG_TU.So_Thang_Cam,
       DANH_SACH_CHUNG_TU.So_Ngay_Le,
       (
           ROUND(
               dbo.Tinh_Tien_Lai(DANH_SACH_CHUNG_TU.Ma_Chung_Tu, GETDATE()),
               -3
           )
       ) AS Tong_Tien_Lai,
       DANH_SACH_CHUNG_TU.Chung_Tu_Goc,
       DANH_SACH_CHUNG_TU.Da_Lam_Lai_Giay,
       DANH_SACH_CHUNG_TU.Tinh_Trang,
       DANH_SACH_CHUNG_TU.Sap_Xep
FROM   (
           SELECT *,
                  dbo.Lay_Thang(CHUNG_TU.Ngay_Ghi_So, GETDATE()) AS So_Thang_Cam,
                  DATEDIFF(DAY, CHUNG_TU.Ngay_Cam_Do, GETDATE()) AS So_Ngay_Cam,
                  dbo.Lay_Ngay(CHUNG_TU.Ngay_Ghi_So, GETDATE()) AS So_Ngay_Le
           FROM   (
                      SELECT ct.Ma_Chung_Tu,
                             (
                                 ct.So_Tien_Cam - ISNULL(
                                     (
                                         SELECT SUM(So_Tien_Thu - So_Tien_Chi)
                                         FROM   CHUNG_TU_THU_CHI cttc
                                         WHERE  cttc.Ma_Chung_Tu_Cam_Do = ct.Ma_Chung_Tu
                                                AND cttc.Ma_Phan_Loai != 'Chuoc'
                                                AND cttc.Ma_Phan_Loai != 'Loi'
                                     ),
                                     0
                                 )
                             ) AS So_Tien,
                             ISNULL(
                                 (
                                     SELECT MAX(cttc.Ngay_Ghi_So)
                                     FROM   CHUNG_TU_THU_CHI cttc
                                     WHERE  cttc.Ma_Chung_Tu_Cam_Do = ct.Ma_Chung_Tu
                                 ),
                                 ct.Ngay
                             ) AS Ngay_Ghi_So,
                             ct.Ngay AS Ngay_Cam_Do,
                             ct.So,
                             ct.Lien,
                             ct.Ten_Khach_Hang,
                             ct.So_CMND,
                             ct.Ngay_Cap_CMND,
                             ct.Noi_Cap,
                             ct.Ngay_Sinh,
                             ct.Dia_Chi,
                             ct.So_Dien_Thoai,
                             ct.Chu_Tiem,
                             ct.So_Tien_Cam,
                             ct.Tu_Ngay,
                             ct.Den_Ngay,
                             ct.Lai_Suat_Ngay,
                             ct.Lai_Suat_Thang,
                             ct.Mat_Giay,
                             ct.Ghi_Chu,
                             CASE 
                                  WHEN cttc.Ma IS NULL THEN 'C'
                                  ELSE 'R'
                             END AS Tinh_Trang,
                             ct.Chung_Tu_Goc,
                             ct.Da_Lam_Lai_Giay,
                             ct.Sap_Xep
                      FROM   CHUNG_TU ct
                             LEFT OUTER JOIN (
                                      SELECT *
                                      FROM   CHUNG_TU_THU_CHI
                                      WHERE  Ma_Phan_Loai = 'Chuoc'
                                  ) cttc
                                  ON  ct.Ma_Chung_Tu = cttc.Ma_Chung_Tu_Cam_Do
                      WHERE  DATEDIFF(DAY, @FromDate, ct.Ngay) &gt;= 0
                             AND DATEDIFF(DAY, @ToDate, ct.Ngay) &lt;= 0
                             AND (
                                     (cttc.Ma IS NULL AND ISNULL(ct.Da_Lam_Lai_Giay, 0) = 0)
                                     OR CAST(1 AS BIT) = @IsAll
                                 )
                  ) CHUNG_TU
       ) DANH_SACH_CHUNG_TU</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="FromDate" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="DateTime" Direction="Input" ParameterName="@FromDate" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="false" AutogeneratedName="ToDate" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="DateTime" Direction="Input" ParameterName="@ToDate" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="false" AutogeneratedName="IsAll" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="Boolean" Direction="Input" ParameterName="@IsAll" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
                  </DbCommand>
                </SelectCommand>
